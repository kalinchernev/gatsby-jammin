{
  "name": "kalinchernev.github.io",
  "description": "SIMPLY a blog.",
  "version": "2.0.0",
  "author": "Kalin Chernev <kalin.chernev@gmail.com> (https://kalinchernev.github.io)",
  "homepage": "https://github.com/kalinchernev/kalinchernev.github.io#readme",
  "license": "MIT",
  "keywords": ["gatsby", "blog"],
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kalinchernev/kalinchernev.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/kalinchernev/kalinchernev.github.io/issues"
  },
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "deploy": "npm run clean && npm run build && gh-pages -d public -b master",
    "clean": "rm -rf public",
    "lint:js": "eslint --ext .js,.jsx --ignore-pattern public,static .",
    "lint:md": "remark content/",
    "write-good": "write-good $(glob 'content/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write"
  },
  "dependencies": {
    "babel-plugin-lodash": "^3.2.11",
    "gatsby": "^1.4.1",
    "gatsby-link": "^1.4.1",
    "gatsby-plugin-catch-links": "^1.0.1",
    "gatsby-plugin-feed": "^1.3.1",
    "gatsby-plugin-google-analytics": "^1.0.1",
    "gatsby-plugin-manifest": "^1.0.1",
    "gatsby-plugin-nprogress": "^1.0.1",
    "gatsby-plugin-offline": "^1.0.1",
    "gatsby-plugin-react-helmet": "^1.0.1",
    "gatsby-plugin-sharp": "^1.4.1",
    "gatsby-plugin-sitemap": "^1.2.0",
    "gatsby-plugin-twitter": "^1.0.5",
    "gatsby-remark-autolink-headers": "^1.4.1",
    "gatsby-remark-copy-linked-files": "^1.4.1",
    "gatsby-remark-images": "^1.3.2",
    "gatsby-remark-prismjs": "^1.2.0",
    "gatsby-remark-responsive-iframe": "^1.4.1",
    "gatsby-source-filesystem": "^1.4.1",
    "gatsby-transformer-remark": "^1.1.0",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.4",
    "react": "^15.6.1",
    "react-disqus-comments": "^1.0.3",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-share": "^1.15.0",
    "react-twitter-widgets": "^1.4.0"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "gh-pages": "^1.0.0",
    "prettier": "^1.5.3",
    "remark-cli": "^4.0.0",
    "remark-preset-lint-recommended": "^3.0.0",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "write-good": "^0.11.3"
  },
  "engines": {
    "node": ">=6.11.2"
  },
  "remarkConfig": {
    "plugins": ["remark-preset-lint-recommended"]
  }
}
